[user]
	name = Ömer Balyali
	name = Ömer Balyali
	username = omerbalyali
	email = omerbalyali@gmail.com

[includeIf "gitdir:/Volumes/Alpha/Projects/Development/"]
	path = ~/.gitconfigs/.gitconfig.personal


[alias]
	a = add
	b = branch
	c = commit
	s = status
	sw = switch
	l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	lg = log --oneline --graph
	lgo = log --oneline
	unstage = reset HEAD --
	last = log -1 HEAD
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[core]
	excludesfile = /Users/alpha/.gitignore_global
[commit]
	template = /Users/alpha/.stCommitMsg
[init]
	defaultBranch = main
